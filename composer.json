{
    "name": "snono/socialite",
    "description": "Laravel wrapper around OAuth 1 & OAuth 2 libraries.",
    "keywords": ["oauth", "laravel"],
    "license": "MIT",
    "homepage": "https://snono.systems",
    "support": {
        "source": "https://github.com/yasergh/socialite"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "../../../vendor/guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/http": "~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.7.0|~5.8.0|^6.0|^7.0",
        "league/oauth1-client": "^1.0"
    },
    "require-dev": {
        "illuminate/contracts": "~5.7.0|~5.8.0|^6.0|^7.0",
        "../../../vendor/mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.7|^3.8|^4.0|^5.0",
        "../../../vendor/phpunit/phpunit": "^7.0|^8.0"
    },

    "autoload": {
        "psr-4": {
            "Snono\\Socialite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Snono\\Socialite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Snono\\Socialite\\SocialiteServiceProvider"
            ],
            "aliases": {
                "Socialite": "Snono\\Socialite\\Facades\\Socialite"
            }
        }
    },

    "scripts": {
        "pre-autoload-dump": [
            "@php -r \"array_map('unlink', glob('bootstrap/cache/*'));\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
